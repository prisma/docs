{
  "name": "prisma2-docs",
  "description": "Prisma.io docs v2.0",
  "version": "1.0.0",
  "author": "Nilufar Bava <bava@prisma.io>",
  "packageManager": "pnpm@8.10.0",
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@percy/agent": "0.28.6",
    "@philpl/buble": "0.19.7",
    "@prisma/client": "2.29.1",
    "@prisma/lens": "0.0.371",
    "@reach/router": "1.3.4",
    "algoliasearch": "^4.13.1",
    "babel-plugin-styled-components": "1.13.3",
    "feather-icons": "^4.29.0",
    "gatsby": "2.32.13",
