    "mdx",
    "typescript",
    "prismaio",
    "howtodata",
    "databases",
    "styled-components"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build --prefix-paths --log-pages",
    "dev": "gatsby develop",
    "serve": "npm run build && gatsby serve --prefix-paths -o",
    "format": "prettier \"**/*.{ts,tsx}\" \"!out\" --write --check",
    "test": "jest",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/tag": "^0.20.3",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "gatsby-remark-sectionize": "^1.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "loader-utils": "^2.0.0",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "ts-jest": "^26.5.5",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
