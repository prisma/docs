{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalDependencies": [".env"],
 
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "outputs": [".next/**", "!.next/cache/**", "cache/**"],
      "env": ["ARGOS_TOKEN", "MIXEDBREAD_API_KEY", "SENTRY_AUTH_TOKEN", "ENABLE_DOCS_SUBDOMAIN_PROXY"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "check": {},
    "types:check": {}
  }
}
