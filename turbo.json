{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [".env"],
 
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "outputs": [".next/**", "!.next/cache/**", "cache/**"],
      "env": ["ARGOS_TOKEN", "MIXEDBREAD_API_KEY"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "check": {},
    "types:check": {}
  }
}
