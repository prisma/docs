{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fetch-openapi": "tsx ./scripts/fetch-openapi.ts",
    "prebuild": "rimraf out",
    "build": "pnpm run fetch-openapi && next build",
    "dev": "next dev",
    "check": "oxfmt . --write && oxlint . --fix",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint:links": "tsx ./scripts/lint-links.ts",
    "lint:code": "tsx ./scripts/lint-code-blocks.ts",
    "lint:spellcheck": "cspell \"content/docs/**/*.mdx\" \"content/docs/**/*.json\""
  },
  "dependencies": {
    "@base-ui/react": "catalog:",
    "@fumadocs/base-ui": "catalog:",
    "@fumadocs/cli": "catalog:",
    "@kapaai/react-sdk": "catalog:",
    "@mixedbread/sdk": "catalog:",
    "@prisma-docs/eclipse": "workspace:^",
    "@prisma-docs/ui": "workspace:*",
    "@radix-ui/react-tooltip": "catalog:",
    "@sentry/nextjs": "catalog:",
    "@streamdown/code": "catalog:",
    "class-variance-authority": "catalog:",
    "dexie": "catalog:",
    "dexie-react-hooks": "catalog:",
    "fumadocs-core": "catalog:",
    "fumadocs-mdx": "catalog:",
    "fumadocs-openapi": "catalog:",
    "fumadocs-ui": "catalog:",
    "jotai": "catalog:",
    "lucide-react": "catalog:",
    "motion": "catalog:",
    "next": "catalog:",
    "next-themes": "catalog:",
    "npm-to-yarn": "catalog:",
    "posthog-js": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "remark-directive": "catalog:",
    "scroll-into-view-if-needed": "catalog:",
    "streamdown": "catalog:",
    "tailwind-merge": "catalog:",
    "use-stick-to-bottom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@argos-ci/playwright": "catalog:",
    "@playwright/test": "catalog:",
    "@tailwindcss/postcss": "catalog:",
    "@types/mdx": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "cspell": "catalog:",
    "fast-glob": "catalog:",
    "gray-matter": "catalog:",
    "next-validate-link": "catalog:",
    "postcss": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:",
