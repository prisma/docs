{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fetch-openapi": "tsx ./scripts/fetch-openapi.ts",
    "prebuild": "rimraf out",
    "build": "pnpm run fetch-openapi && next build",
    "dev": "next dev",
    "check": "oxfmt . --write && oxlint . --fix",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint:links": "tsx ./scripts/lint-links.ts",
    "lint:code": "tsx ./scripts/lint-code-blocks.ts",
    "lint:spellcheck": "cspell \"content/docs/**/*.mdx\" \"content/docs/**/*.json\""
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@fumadocs/base-ui": "16.6.1",
    "@fumadocs/cli": "^1.2.4",
    "@kapaai/react-sdk": "^0.9.1",
    "@mixedbread/sdk": "^0.46.0",
    "@prisma-docs/eclipse": "workspace:^",
    "@prisma-docs/ui": "workspace:*",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.38.0",
    "@streamdown/code": "^1.0.1",
    "class-variance-authority": "^0.7.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "fumadocs-core": "16.6.1",
    "fumadocs-mdx": "14.2.7",
    "fumadocs-openapi": "^10.3.5",
    "fumadocs-ui": "npm:@fumadocs/base-ui@16.6.1",
    "jotai": "^2.17.0",
    "lucide-react": "^0.562.0",
    "motion": "^12.29.2",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "npm-to-yarn": "^3.0.1",
    "posthog-js": "^1.343.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remark-directive": "^4.0.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "streamdown": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@argos-ci/playwright": "^6.4.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "cspell": "^8.0.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "next-validate-link": "^1.6.3",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
